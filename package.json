{
  "name": "querypie-docs",
  "version": "25.08.1",
  "private": true,
  "description": "QueryPie Docs",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "postbuild": "npm run generate-pagefind && npm run generate-sitemap",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "npx prettier --write .",
    "generate-pagefind": "pagefind --site .next/server/app --output-path public/_pagefind",
    "generate-sitemap": "node scripts/generate-sitemap/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/querypie/querypie-docs.git"
  },
  "author": "QueryPie",
  "bugs": {
    "url": "https://github.com/querypie/querypie-docs/issues"
  },
  "homepage": "https://github.com/querypie/querypie-docs#readme",
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@next/third-parties": "^15.5.3",
    "@theguild/remark-mermaid": "^0.3.0",
    "gray-matter": "^4.0.3",
    "next": "^15.5.3",
    "nextra": "^4.4.0",
    "nextra-theme-docs": "^4.4.0",
    "pino": "^9.9.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "remark-emoji": "^5.0.2",
    "remark-gfm": "^4.0.1",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.4.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-next": "^15.5.3",
    "eslint-plugin-unused-imports": "^4.2.0",
    "jsdom": "^27.0.0",
    "pagefind": "^1.4.0",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
